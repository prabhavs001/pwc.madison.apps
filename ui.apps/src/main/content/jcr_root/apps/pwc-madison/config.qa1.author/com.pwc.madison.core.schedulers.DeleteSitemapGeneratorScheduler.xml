    jcr:primaryType="sling:OsgiConfig"
    scheduler.name="Delete Sitemap Generation Scheduler"
    service.enabled="{Boolean}true"
    scheduler.concurrent="{Boolean}true"
    scheduler.expression="0 15 1,5,9,13,17,21 ? * * *"
