<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:primaryType="nt:unstructured"
        jcr:title="Citation Report"
        sling:resourceType="granite/ui/components/coral/foundation/page">
        <head jcr:primaryType="nt:unstructured">
            <viewport
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/admin/page/viewport"/>
            <favicon
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/page/favicon"/>
            <clientlibs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/includeclientlibs"
                categories="[granite.ui.foundation,cq.gui.damadmin.reviewtask,cq.projects.admin,apps.pwc.report.citation-report]"/>
        </head>
        <body
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/page/body">
            <items jcr:primaryType="nt:unstructured">
                <form
                    granite:class="hide-steps coral-Form--vertical foundation-form mode-default citation-report-form"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form"
                    foundationForm="{Boolean}false"
                    foundationFormAjax="{Boolean}false"
                    maximized="{Boolean}true"
                    novalidate="{Boolean}true"
                    style="vertical">
                    <items jcr:primaryType="nt:unstructured">
                        <wizard
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Autolinker Tool"
                            sling:resourceType="granite/ui/components/coral/foundation/wizard"
                            cancelHref="${empty header.Referer ? granite:concat(&quot;/assets.html&quot;, granite:encodeURIPath(requestPathInfo.suffix)) : header.Referer}">
                            <items jcr:primaryType="nt:unstructured">
                                <step1
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Citation Report"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                    <items jcr:primaryType="nt:unstructured">
                                        <properties
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container"
                                            active="{Boolean}true"
                                            class="project-collectionsettings"
                                            rel="project-taskmanagement-addtaskwizard"
                                            title="Citation Report">
                                            <items jcr:primaryType="nt:unstructured">
                                                <citation-report
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="pwc-madison/components/commons/citation-report"/>
                                                <autolink-button
                                                    granite:class="foundation-wizard-control"
                                                    granite:rel="autolink-button"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/button"
                                                    text="Autolink"/>
                                            </items>
                                        </properties>
                                    </items>
                                    
                                </step1>
                            </items>
                        </wizard>
                    </items>
                </form>
            </items>
        </body>
    </jcr:content>
</jcr:root>
