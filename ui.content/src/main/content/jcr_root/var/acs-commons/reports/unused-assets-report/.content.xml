<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:designPath="/etc/designs/acs-commons"
        jcr:description="A report to display assets which are not referenced in DITA Content and Web Content."
        jcr:primaryType="nt:unstructured"
        jcr:title="Unused Assets Report"
        sling:resourceType="acs-commons/components/utilities/report-builder/report-page">
        <config
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/parsys">
            <unusedassets
                jcr:primaryType="nt:unstructured"
                sling:resourceType="pwc-madison/components/report/unusedassets"
                pageSize="50"
                reportExecutor="com.pwc.madison.core.reports.UnusedAssetsReportExecutor"/>
        </config>
        <parameters
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/parsys">
            <pathfield
                jcr:primaryType="nt:unstructured"
                sling:resourceType="pwc-madison/components/report/pathfield"
                fieldLabel="Path"
                name="path"
                required="true"
                resourceType="granite/ui/components/foundation/form/pathbrowser"
                rootPath="/content/dam/pwc-madison"/>
            <reportmessage
                jcr:primaryType="nt:unstructured"
                sling:resourceType="pwc-madison/components/report/reportmessage"
                message="&lt;p>This report execution is Resource Intensive.&lt;/p>&#xd;&#xa;&lt;p>Please select a folder which has smaller set of assets.&lt;/p>&#xd;&#xa;&lt;p>Eg: /content/dam/pwc-madison/ditaroot/us/en/pwc/alerts&lt;/p>&#xd;&#xa;&lt;p>If the entire set of results are not visible as per the total number of records, please use the &amp;quot;Download Report&amp;quot; Button to get all the results.&lt;/p>&#xd;&#xa;"
                messageHeading="Warning"
                resourceType="pwc-madison/components/report/reportmessage"
                textIsRich="true"/>
        </parameters>
        <columns
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/parsys">
            <assetpath
                jcr:primaryType="nt:unstructured"
                sling:resourceType="pwc-madison/components/report/assetpath"
                exporter="com.adobe.acs.commons.reports.models.PathReportCellCSVExporter"
                heading="Path"
                link="true"/>
            <text
                jcr:primaryType="nt:unstructured"
                sling:resourceType="acs-commons/components/utilities/report-builder/columns/text"
                exporter="com.adobe.acs.commons.reports.models.StringReportCellCSVExporter"
                heading="Title"
                property="jcr:content/metadata/dc:title"/>
            <assettype
                jcr:primaryType="nt:unstructured"
                sling:resourceType="pwc-madison/components/report/assettype"
                exporter="com.pwc.madison.core.reports.AssetTypeReportCellCSVExporter"
                heading="Type of Asset"/>
            <text_1798402270
                jcr:primaryType="nt:unstructured"
                sling:resourceType="acs-commons/components/utilities/report-builder/columns/text"
                exporter="com.adobe.acs.commons.reports.models.StringReportCellCSVExporter"
                heading="Document State"
                property="jcr:content/metadata/docstate"/>
            <text_1048873367
                jcr:primaryType="nt:unstructured"
                sling:resourceType="acs-commons/components/utilities/report-builder/columns/text"
                exporter="com.adobe.acs.commons.reports.models.StringReportCellCSVExporter"
                heading="Content State"
                property="jcr:content/metadata/pwc-content-status"/>
            <date
                jcr:primaryType="nt:unstructured"
                sling:resourceType="acs-commons/components/utilities/report-builder/columns/date"
                exporter="com.adobe.acs.commons.reports.models.DateReportCellCSVExporter"
                format="dd-MM-yyyy"
                heading="Last Modified Date"
                property="jcr:content/jcr:lastModified"/>
            <date_1289015004
                jcr:primaryType="nt:unstructured"
                sling:resourceType="acs-commons/components/utilities/report-builder/columns/date"
                exporter="com.adobe.acs.commons.reports.models.DateReportCellCSVExporter"
                format="dd-MM-yyyy"
                heading="Published Date"
                property="jcr:content/cq:lastReplicated"/>
            <text_1215312226
                jcr:primaryType="nt:unstructured"
                sling:resourceType="acs-commons/components/utilities/report-builder/columns/text"
                exporter="com.adobe.acs.commons.reports.models.StringReportCellCSVExporter"
                heading="Checked Out?"
                property="jcr:isCheckedOut"/>
            <text_1005694252
                jcr:primaryType="nt:unstructured"
                sling:resourceType="acs-commons/components/utilities/report-builder/columns/text"
                exporter="com.adobe.acs.commons.reports.models.StringReportCellCSVExporter"
                heading="Checked Out By"
                property="jcr:content/cq:drivelock"/>
            <text_1253627985
                jcr:primaryType="nt:unstructured"
                sling:resourceType="acs-commons/components/utilities/report-builder/columns/text"
                exporter="com.adobe.acs.commons.reports.models.StringReportCellCSVExporter"
                heading="Creator Name"
                property="jcr:createdBy"/>
        </columns>
    </jcr:content>
</jcr:root>
