                <metaData
                    jcr:primaryType="nt:unstructured"
                    PROCESS="com.adobe.fmdita.custom.workflow.customsteps.AEMSiteXrefIndexStore"
                    PROCESS_AUTO_ADVANCE="true"/>
            </process_1298740092>
            <process
                jcr:primaryType="nt:unstructured"
                jcr:title="PwC Madison Post Processing"
                sling:resourceType="cq/workflow/components/model/process">
                <metaData
                    jcr:primaryType="nt:unstructured"
                    PROCESS="com.pwc.madison.core.workflows.UpdateRelatedContent"
                    PROCESS_AUTO_ADVANCE="true"/>
            </process>
			<process_2009303999
                jcr:primaryType="nt:unstructured"
                jcr:title="Create Chapter level TOC JSON"
                sling:resourceType="cq/workflow/components/model/process">
                <metaData
                    jcr:primaryType="nt:unstructured"
                    PROCESS="com.pwc.madison.core.workflows.CreateChapterTocJsonWorkflow"
                    PROCESS_AUTO_ADVANCE="true"/>
            </process_2009303999>
            <process_734086778
                jcr:primaryType="nt:unstructured"
                jcr:title="PwC Madison Replicate Pages"
