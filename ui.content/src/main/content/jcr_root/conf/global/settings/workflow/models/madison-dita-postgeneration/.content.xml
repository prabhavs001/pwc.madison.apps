                sling:resourceType="cq/workflow/components/model/process">
                <metaData
                    jcr:primaryType="nt:unstructured"
                    PROCESS="com.pwc.madison.core.workflows.PurgeDuplicatePagePaths"
                    PROCESS_AUTO_ADVANCE="true"/>
            </process_544956105>
            <process_2076890889
                jcr:description="Process to fix all broken peer links in syndicated content"
                jcr:primaryType="nt:unstructured"
                jcr:title="PwC Madison Syndication Peer Link Fix Process"
                sling:resourceType="cq/workflow/components/model/process">
                <metaData
                    jcr:primaryType="nt:unstructured"
                    PROCESS="com.pwc.madison.core.workflows.SyndicationPeerLinkFixProcess"
                    PROCESS_AUTO_ADVANCE="true"/>
            </process_2076890889>
            <process_1298740092
