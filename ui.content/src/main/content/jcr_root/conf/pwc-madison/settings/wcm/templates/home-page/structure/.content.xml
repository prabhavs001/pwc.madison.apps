<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[mobile/groups/responsive]"
        cq:template="/conf/pwc-madison/settings/wcm/templates/home-page"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="pwc-madison/components/structure/page-vp">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <page_maincontainer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="pwc-madison/components/structure/page-maincontainer">
                <maincontainer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid">
                    <header
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="pwc-madison/components/structure/header/v1/header"
                        editable="{Boolean}true">
                    </header>
                    <alert
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="pwc-madison/components/content/home/alert"
                        editable="{Boolean}true"/>
                    <hero_tile
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="pwc-madison/components/content/home/hero-tile"
                        editable="{Boolean}true"/>
                    <featured_content
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="pwc-madison/components/content/home/featured-content"
                        editable="{Boolean}true"/>
                    <home_page_module
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="pwc-madison/components/structure/home-page-module">
                        <newscontainer
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/foundation/components/responsivegrid">
                            <columncontrol
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="pwc-madison/components/structure/frog-home/columncontrol"
                                pageType="homePage">
                                <columnControl jcr:primaryType="nt:unstructured">
                                    <item0
                                        jcr:primaryType="nt:unstructured"
                                        column="8"/>
                                    <item1
                                        jcr:primaryType="nt:unstructured"
                                        column="4"/>
                                </columnControl>
                                <parsys-0
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="wcm/foundation/components/responsivegrid">
                                    <most_popular
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="pwc-madison/components/content/home/most-popular"
                                        editable="{Boolean}true"/>
                                </parsys-0>
                                <parsys-1
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="wcm/foundation/components/responsivegrid">
                                    <webcast_podcast
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="pwc-madison/components/content/home/webcast-&amp;-podcast"
                                        editable="{Boolean}true"/>
                                    <news_list
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="pwc-madison/components/content/home/news-list"
                                        editable="{Boolean}true"/>
                                </parsys-1>
                            </columncontrol>
                        </newscontainer>
                    </home_page_module>
                    <insights
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="pwc-madison/components/content/home/insights"
                        editable="{Boolean}true"/>
                    <recently_viewed
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="pwc-madison/components/content/home/recently-viewed"
                        editable="{Boolean}true"/>
                    <footer
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="pwc-madison/components/structure/footer/v1/footer"
                        editable="{Boolean}true"/>
                </maincontainer>
            </page_maincontainer>
            <search_modal
                jcr:primaryType="nt:unstructured"
                sling:resourceType="pwc-madison/components/content/search-modal"/>
            <user_registration_modal
                jcr:primaryType="nt:unstructured"
                sling:resourceType="pwc-madison/components/content/user-registration-modals"/>
            <tutorial_modal
                jcr:primaryType="nt:unstructured"
                sling:resourceType="pwc-madison/components/content/tutorial-modal"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}650"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
