<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[/etc/mobile/groups/responsive]"
        cq:template="/conf/pwc-madison/settings/wcm/templates/article-structured-template"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="pwc-madison/components/structure/page-vp-inloop">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <page_maincontainer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="pwc-madison/components/structure/page-maincontainer">
                <maincontainer
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                    <header
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="pwc-madison/components/structure/header/v1/header">
                        <global_navigation
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="pwc-madison/components/structure/global-navigation"/>
                    </header>
                    <fixed_container
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/foundation/components/responsivegrid">
                        <toolbar
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="pwc-madison/components/inloop/toolbar"/>
                        <progress-indicator
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="pwc-madison/components/inloop/progress-indicator"
                                editable="{Boolean}true"/>
                    </fixed_container>
                    <article_structured_content
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/foundation/components/responsivegrid">
                        <teaser_container
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="wcm/foundation/components/responsivegrid">
                            <teaser
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="pwc-madison/components/inloop/teaser"
                                    editable="{Boolean}true"/>
                        </teaser_container>
                        <inloop_download_container
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="wcm/foundation/components/responsivegrid"
                                editable="{Boolean}true">
                        </inloop_download_container>
                        <inloop_main_container
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="wcm/foundation/components/responsivegrid"
                                editable="{Boolean}true">
                            <inloop_flexible_container
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="pwc-madison/components/core/container"
                                editable="{Boolean}true">
                        	</inloop_flexible_container>
	                        <inloop_content_container
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="pwc-madison/components/core/container"
                                editable="{Boolean}true">
                            </inloop_content_container>
                        </inloop_main_container>
                    </article_structured_content>
                    <footer
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="pwc-madison/components/structure/footer/v1/footer"/>
                </maincontainer>
            </page_maincontainer>
            <search_modal
                jcr:primaryType="nt:unstructured"
                sling:resourceType="pwc-madison/components/content/search-modal"/>
            <user_registration_modal
                jcr:primaryType="nt:unstructured"
                sling:resourceType="pwc-madison/components/content/user-registration-modals"/>
            <multimedia_modal
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="pwc-madison/components/inloop/multimedia-modal"/>
            <tutorial_modal
                jcr:primaryType="nt:unstructured"
                sling:resourceType="pwc-madison/components/content/tutorial-modal"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}650"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
