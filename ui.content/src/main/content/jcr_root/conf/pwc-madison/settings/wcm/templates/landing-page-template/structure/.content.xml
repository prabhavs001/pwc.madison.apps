<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[/etc/mobile/groups/responsive]"
        cq:template="/conf/pwc-madison/settings/wcm/templates/landing-page-template"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="pwc-madison/components/structure/page-vp">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <page_maincontainer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="pwc-madison/components/structure/page-maincontainer">
                <maincontainer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid">
                    <header
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="pwc-madison/components/structure/header/v1/header">
                        <global_navigation
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="pwc-madison/components/structure/global-navigation"/>
                    </header>
                    <breadcrumb
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="pwc-madison/components/content/breadcrumb"/>
                    <overview
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="pwc-madison/components/content/overview"
                        editable="{Boolean}true"/>
		    <hero_tile
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="pwc-madison/components/content/home/hero-tile"
                        editable="{Boolean}true"/>	
                    <responsivegrid
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"
                        editable="{Boolean}true">
                        <cq:responsive jcr:primaryType="nt:unstructured"/>
                    </responsivegrid>
                    <reference_links
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="pwc-madison/components/content/reference-links"
                        editable="{Boolean}true"/>
                    <sme_contact
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="pwc-madison/components/content/sme-contact"
                        editable="{Boolean}true"/>
                    <footer
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="pwc-madison/components/structure/footer/v1/footer"/>
                </maincontainer>
            </page_maincontainer>
            <search_modal
                jcr:primaryType="nt:unstructured"
                sling:resourceType="pwc-madison/components/content/search-modal"/>
            <user_registration_modal
                jcr:primaryType="nt:unstructured"
                sling:resourceType="pwc-madison/components/content/user-registration-modals"/>
            <tutorial_modal
                jcr:primaryType="nt:unstructured"
                sling:resourceType="pwc-madison/components/content/tutorial-modal"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}650"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
